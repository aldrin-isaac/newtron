{
  "version": "1.0",
  "description": "4-node topology: 2 spines + 2 leaves (full mesh)",
  "devices": {
    "spine1": {
      "device_config": {
        "route_reflector": true
      },
      "interfaces": {
        "Ethernet0": {
          "link": "leaf1:Ethernet0",
          "service": "transit",
          "ip": "10.1.0.0/31"
        },
        "Ethernet1": {
          "link": "leaf2:Ethernet0",
          "service": "transit",
          "ip": "10.1.0.2/31"
        }
      }
    },
    "spine2": {
      "device_config": {
        "route_reflector": true
      },
      "interfaces": {
        "Ethernet0": {
          "link": "leaf1:Ethernet1",
          "service": "transit",
          "ip": "10.1.0.4/31"
        },
        "Ethernet1": {
          "link": "leaf2:Ethernet1",
          "service": "transit",
          "ip": "10.1.0.6/31"
        }
      }
    },
    "leaf1": {
      "interfaces": {
        "Ethernet0": {
          "link": "spine1:Ethernet0",
          "service": "transit",
          "ip": "10.1.0.1/31"
        },
        "Ethernet1": {
          "link": "spine2:Ethernet0",
          "service": "transit",
          "ip": "10.1.0.5/31"
        },
        "Ethernet2": {},
        "Ethernet3": {}
      }
    },
    "leaf2": {
      "interfaces": {
        "Ethernet0": {
          "link": "spine1:Ethernet1",
          "service": "transit",
          "ip": "10.1.0.3/31"
        },
        "Ethernet1": {
          "link": "spine2:Ethernet1",
          "service": "transit",
          "ip": "10.1.0.7/31"
        },
        "Ethernet2": {},
        "Ethernet3": {}
      }
    }
  },
  "links": [
    {"a": "spine1:Ethernet0", "z": "leaf1:Ethernet0"},
    {"a": "spine1:Ethernet1", "z": "leaf2:Ethernet0"},
    {"a": "spine2:Ethernet0", "z": "leaf1:Ethernet1"},
    {"a": "spine2:Ethernet1", "z": "leaf2:Ethernet1"}
  ],
  "newtlab": {
    "link_port_base": 10000,
    "console_port_base": 12000,
    "ssh_port_base": 13000
  }
}
