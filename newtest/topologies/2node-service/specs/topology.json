{
  "version": "1.0",
  "description": "2-node topology with service-defined interfaces for intent-based health check validation",
  "devices": {
    "switch1": {
      "interfaces": {
        "Ethernet0": { "link": "switch2:Ethernet0", "service": "transit", "ip": "10.1.0.0/31" },
        "Ethernet1": { "link": "host1:eth0", "service": "local-irb", "ip": "10.100.1.1/24" },
        "Ethernet2": { "link": "host2:eth0", "service": "local-bridge" },
        "Ethernet3": { "link": "host3:eth0", "service": "l2-extend" },
        "Ethernet4": { "link": "switch2:Ethernet4" },
        "Ethernet5": { "link": "switch2:Ethernet5" }
      }
    },
    "switch2": {
      "interfaces": {
        "Ethernet0": { "link": "switch1:Ethernet0", "service": "transit", "ip": "10.1.0.1/31" },
        "Ethernet1": { "link": "host4:eth0", "service": "local-irb", "ip": "10.100.1.1/24" },
        "Ethernet2": { "link": "host5:eth0", "service": "local-bridge" },
        "Ethernet3": { "link": "host6:eth0", "service": "l2-extend" },
        "Ethernet4": { "link": "switch1:Ethernet4" },
        "Ethernet5": { "link": "switch1:Ethernet5" }
      }
    },
    "host1": { "type": "host", "interfaces": { "eth0": { "link": "switch1:Ethernet1" } } },
    "host2": { "type": "host", "interfaces": { "eth0": { "link": "switch1:Ethernet2" } } },
    "host3": { "type": "host", "interfaces": { "eth0": { "link": "switch1:Ethernet3" } } },
    "host4": { "type": "host", "interfaces": { "eth0": { "link": "switch2:Ethernet1" } } },
    "host5": { "type": "host", "interfaces": { "eth0": { "link": "switch2:Ethernet2" } } },
    "host6": { "type": "host", "interfaces": { "eth0": { "link": "switch2:Ethernet3" } } }
  },
  "newtlab": {
    "link_port_base": 10000,
    "console_port_base": 12000,
    "ssh_port_base": 13000
  }
}
