{
  "version": "1.0",
  "description": "2-node topology: 1 spine + 1 leaf",
  "devices": {
    "spine1": {
      "device_config": {
        "route_reflector": true
      },
      "interfaces": {
        "Ethernet0": {
          "link": "leaf1:Ethernet0",
          "service": "transit",
          "ip": "10.1.0.0/31"
        },
        "Ethernet1": {
          "link": "leaf1:Ethernet1"
        },
        "Ethernet2": {
          "link": "leaf1:Ethernet2"
        }
      }
    },
    "leaf1": {
      "interfaces": {
        "Ethernet0": {
          "link": "spine1:Ethernet0",
          "service": "transit",
          "ip": "10.1.0.1/31"
        },
        "Ethernet1": {
          "link": "spine1:Ethernet1"
        },
        "Ethernet2": {
          "link": "spine1:Ethernet2"
        }
      }
    }
  },
  "links": [
    {"a": "spine1:Ethernet0", "z": "leaf1:Ethernet0"},
    {"a": "spine1:Ethernet1", "z": "leaf1:Ethernet1"},
    {"a": "spine1:Ethernet2", "z": "leaf1:Ethernet2"}
  ],
  "newtlab": {
    "link_port_base": 10000,
    "console_port_base": 12000,
    "ssh_port_base": 13000
  }
}
